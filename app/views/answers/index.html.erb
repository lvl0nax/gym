<h1>Listing answers</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Question</th>
    <th></th>

  </tr>

<% @answers.each do |answer| %>
  <tr>
    <td><%= answer.content %></td>
    <td><%= answer.question %></td>
    <td><%= link_to "desctoy", answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>

  </tr>
<% end %>
</table>

<br />


