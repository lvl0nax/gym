<h1>Listing txtinfos</h1>
<%= link_to 'New Txtinfo', new_txtinfo_path %>

<table>
  <tr>
    <th>Tag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @txtinfos.each do |txtinfo| %>
  <tr>
    <td><%= txtinfo.tag %></td>
    <td><%= link_to 'Show', txtinfo %></td>
    <td><%= link_to 'Edit', edit_txtinfo_path(txtinfo) %></td>
    <td><%= link_to 'Destroy', txtinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

